        android:layout_alignParentBottom="true"
        android:layout_marginEnd="217dp"
        android:layout_marginBottom="317dp"
        android:text="Tambahkan" />

    <EditText
        android:id="@+id/namasiswa"
        android:layout_width="wrap_content"
